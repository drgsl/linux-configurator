
# yay
sudo pacman -S --needed git base-devel && git clone https://aur.archlinux.org/yay.git && cd yay && makepkg -Si

# installs using yay
yay -S microsoft-edge-stable google-chrome oh-my-zsh-git 
yay -S google-chrome-dev google-chrome-beta
yay -S microsoft-edge-dev-bin microsoft-edge-beta-bin
yay -S vscodium


